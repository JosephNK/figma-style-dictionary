{
  "scss": {
    "transforms": [
      "attribute/cti",
      "name/cti/kebab",
      "time/seconds",
      "content/icon",
      "size/rem",
      "color/css",
      "typography/scss/gen"
    ],
    "buildPath": "_build/scss/",
    "prefix": "ds",
    "files": [{
      "destination": "design-system-color.scss",
      "format": "scss/variables",
      "type": "color",
      "filter": "filter-common-color",
      "options": {
        "fileHeader": "fileCommentsHeader"
      },
      "destination": "design-system-typography.scss",
      "format": "scss/variables",
      "type": "typography",
      "filter": "filter-scss-font",
      "options": {
        "fileHeader": "fileCommentsHeader"
      }
    }]
  }
}